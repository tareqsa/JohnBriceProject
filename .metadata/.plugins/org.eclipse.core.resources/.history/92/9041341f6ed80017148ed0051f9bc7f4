package exceptions;

// TODO: Auto-generated Javadoc
/**
 * The Enum ExceptionType.
 */
public enum ExceptionType
{
	
	/** The Class not found exception. */
	ClassNotFoundException,
	
	/** The SQL exception. */
	SQLException,
	
	/** The Interrupted exception. */
	InterruptedException,
	
	/** The Duplicate entry exception. */
	DuplicateEntryException,
	
	/** The Wrong data input exception. */
	WrongDataInputException,
	
	/** The Null connection exception. */
	NullConnectionException,
	
	/** The Parse exception. */
	ParseException,
	
	/** The Duplicate coupon type exception. */
	DuplicateCouponTypeException,
	
	/** The Un available coupon exception. */
	UnAvailableCouponException,
	
	/** The Empty entry exception. */
	EmptyEntryException

}