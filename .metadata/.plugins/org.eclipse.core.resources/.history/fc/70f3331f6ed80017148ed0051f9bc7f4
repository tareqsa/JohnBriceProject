package exceptions;

/**
 * 
 * An exception that provides information on empty information in database
 *
 */
public class EmptyEntryException extends Exception
{

	/**
	 * Constructs a new exception with null as its detail message
	 */
	public EmptyEntryException()
	{

	}

	/**
	 * Constructs a new exception with the specified detail message.
	 * @param message message the detail message. The detail message is saved for later retrieval by the Throwable.getMessage() method.
	 */
	public EmptyEntryException(String message)
	{
		super(message);
	}

}